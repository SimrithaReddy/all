<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>FIRST EJS APP</title>
    <link rel="stylesheet" href="style.css">
</head>

<body>

    <h1>FIRST EJS APP</h1>


    <% user.forEach(userdata=>{ %>
        <div class="user">
            <h1>
                <%= userdata.name %>
            </h1>
            <img src="<%= userdata.image%>" alt="my">
            <p>
                <%= userdata.email %>
            </p>
        </div>

        <% }) %>
<br>
      <div>
        <a href="/form">Add User</a>
        <a href="/remove/user">Remove User</a>
      </div>

</body>

</html>